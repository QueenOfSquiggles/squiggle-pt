config_version=5

[DynamicContentLoading]

links/webpage_root="https://queenofsquiggles.github.io/data/"
links/game_id="squiggle-pt"
cache/use_cache=true
cache/content_auto_refresh=true

[application]

config/name="Gruesome Claims"
config/description="A small PT-like game for testing out my game base and finding opportunities to expand upon it"
run/main_scene="res://LaunchAnimation/launch_sequence.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

DynamicContentManager="*res://addons/dynamic-web-content/autoload/dynamic_content_manager.gd"
Scenes="*res://addons/squiggle_zone/autoload/scenes.tscn"
BGM="*res://addons/squiggle_zone/autoload/bgm.tscn"

[dotnet]

project/assembly_name="GodotCSharpHelpers"

[editor_plugins]

enabled=PackedStringArray("res://addons/dynamic-web-content/plugin.cfg", "res://addons/squiggle_zone/plugin.cfg")

[gui]

theme/custom="res://Assets/Theme/default_theme.tres"
theme/custom_font="res://Assets/Fonts/DelaGothicOne-Regular.ttf"

